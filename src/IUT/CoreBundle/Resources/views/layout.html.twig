<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="administration base d'images">
    <meta name="author" content="erobot">
    <link rel="icon" href="">

    <title>{% block title %}{% endblock %}</title>


    {% block stylesheetsbootstrap %}
        <!-- Font Awesome -->
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">

        <link rel="stylesheet" href="{{ asset('bundles/iutadmin/css/bootstrap.min.css') }}">

        <link rel="stylesheet" href="{{ asset('bundles/iutadmin/css/mdb.min.css') }}">


    {% endblock %}

    {% block stylesheets %}
        {% stylesheets
            'bundles/iutcore/css/main.css'
            filter='cssrewrite' %}
            <link rel="stylesheet" href="{{ asset_url }}"/>
        {% endstylesheets %}
    {% endblock %}
</head>

<body id="page-top">


{% block header %}

    <header>
        <nav class="navbar navbar-expand-lg navbar-dark fixed-top scrolling-navbar smooth-scroll">
            <div class="container">

                <a class="navbar-brand " href="#page-top">Erobot</a>

                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#basicExampleNav"
                        aria-controls="basicExampleNav"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse" id="basicExampleNav">

                    <ul class="navbar-nav ml-auto  mx-5">

                        <li class="nav-item mx-2">
                            <a class="nav-link" href="#page-top">Accueil</a>
                        </li>
                        <li class="nav-item mx-2">
                            <a class="nav-link" href="#team">Pages Perso</a>
                        </li>
                        <li class="nav-item mx-2">
                            <a class="nav-link" href="#contact">Contact</a>
                        </li>
                        <li class="nav-item mx-2">
                            <a class="nav-link" href="{{ path("iut_admin_homepage") }}">Base de donnée</a>
                        </li>
                    </ul>
                </div>

            </div>
        </nav>

        <div id="intro" class="view"
             style="background-image: url({{ asset('bundles/iutcore/css/photo_iut.jpg') }});  -webkit-background-size: cover;
                     -moz-background-size: cover;
                     -o-background-size: cover;
                     background-size: cover;">

            <div class="full-bg-img">

                <div class="mask rgba-black-light">
                    <div class="mask rgba-black-light">
                        <div class="container-fluid d-flex align-items-center justify-content-center h-100">

                            <div class="row d-flex justify-content-center text-center">

                                <div class="col-md-10">

                                    <h2 class="display-3 font-weight-bold white-text mb-2">Erobot</h2>

                                    <hr class="hr-light">

                                    <h4 class="white-text my-4">Reconnaissance d'images de monnaies</h4>
                                    <!--href="https://iut.lyberteam.eu/"-->

                                    <button type="button" class="btn btn-outline-white"
                                            onclick="toastr.info('Bientôt disponible, en attende de la partie algorythimique');">
                                        Tester en ligne<i
                                                class="fa fa-cloud ml-2"></i></button>


                                    <a href="{{ path("iut_admin_homepage") }}">
                                        <button type="button" class="btn btn-outline-white">Administrer la base de
                                            donnée<i
                                                    class="fa fa-database ml-2"></i></button>
                                    </a>

                                    {% for label, messages in app.flashes %}
                                        {% for message in messages %}


                                            {% if message == 'Veuillez corriger les informations' %}
                                                <a href="#contact">
                                                    <div class="alert alert-warning mt-3" role="alert">
                                                        {{ message }}
                                                    </div>
                                                </a>
                                            {% else %}

                                                <div class="alert alert-warning mt-3" role="alert">
                                                    {{ message }}
                                                </div>
                                            {% endif %}

                                        {% endfor %}
                                    {% endfor %}
                                </div>

                            </div>
                        </div>
                    </div>
                </div>


            </div>

        </div>
    </header>
{% endblock %}



{% block main %}



{% endblock %}


{% block footer %}

    <footer>
        <div class="container text-white text-center ">
            <div class="row py-4 ">
                <div class="col-md-4"><br>
                    <span class="copyright">Copyright &copy; Erobot 2018</span>
                </div>
                <div class="col-md-4">
                    <ul class="list-inline social-buttons">
                        <li class="list-inline-item smooth-scroll">

                            <a href="#page-top" type="button" class="btn-floating btn-lg  "><i
                                        class="fa fa-angle-double-up"></i></a>
                        </li>
                    </ul>
                </div>
                <div class="col-md-4"><br>
                    <ul class="list-inline quicklinks">
                        <li class="list-inline-item">
                            <a class="text-white"
                               href="http://www.iut-orsay.u-psud.fr/fr/formations/dut/info_dut_as.html">Projet de l'IUT
                                d'Orsay Informatique</a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </footer>


{% endblock %}

{% block javascripts %}
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js"
            integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q"
            crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"
            integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl"
            crossorigin="anonymous"></script>
    {% javascripts '@IUTAdminBundle/Resources/public/js/mdb.min.js' %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
    <script src="https://maps.google.com/maps/api/js?key=AIzaSyCcgzUTYDUG0Nj9WwTuBvyMTbzyL8BF1jQ"></script>

    <script>
        function regular_map() {
            var var_location = new google.maps.LatLng(48.7104831, 2.1624122);

            var var_mapoptions = {
                center: var_location,
                zoom: 14
            };

            var var_map = new google.maps.Map(document.getElementById("map-container"),
                var_mapoptions);

            var var_marker = new google.maps.Marker({
                position: var_location,
                map: var_map,
                title: "Notre IUT"
            });
        }

        google.maps.event.addDomListener(window, 'load', regular_map);
    </script>


{% endblock %}

</body>
</html>
